{
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "jest": true
    },
    "rules": {
        "function-paren-newline": 0,
        "comma-dangle": ["error", "never"],
        "no-use-before-define": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}